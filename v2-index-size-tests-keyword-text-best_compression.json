{
  "template": "v2-index-size-tests-keyword-text-best_compression",
  "settings": {
    "index.refresh_interval": "5s",
    "index.codec": "best_compression",
    "number_of_shards": 1,
    "number_of_replicas": 0
  },
  "mappings": {
    "doc": {
      "dynamic_templates": [{
        "string_fields": {
          "mapping": {
            "norms": {
              "enabled": false
            },
            "type": "string",
            "index": "analyzed",
            "fields": {
              "keyword": {
                "ignore_above": 256,
                "type": "string",
                "index": "not_analyzed",
                "doc_values": true
              }
            }
          },
          "match_mapping_type": "string",
          "match": "*"
        }
      }],
      "properties": {
      },
      "_all": {
        "enabled": false
      }
    }
  },
  "aliases": {}
}
